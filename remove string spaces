def no_space(x):
    return x.replace(" ", "")
input_string1 = "8 j 8   mBliB8g  imjB8B8  jl  B"
result1 = no_space(input_string1)
print(result1)  

input_string2 = "8 8 Bi fk8h B 8 BB8B B B  B888 c hl8 BhB fd"
result2 = no_space(input_string2)
print(result2)  

input_string3 = "8aaaaa dddd r     "
result3 = no_space(input_string3)
print(result3)